<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="fr.unicaen.master.dnr2i.cc.library.relationship">

    <class name="PersistentRelationship" table="relationship">
        <id name="id">
            <generator class="increment"/>
        </id>
        <property name="relation"/>
    </class>

</hibernate-mapping>
